Item FB tristate
Item FB_FOOBAR boolean
Item FB_BARFOO boolean
Depends FB_FOOBAR "FB=y || FB=n"
Depends FB_BARFOO "FB!=n"

Item	ISDN_CAPI	tristate	menuconfig

Item	GIGASET_CAPI	boolean
Depends	GIGASET_CAPI	"ISDN && ISDN_DRV_GIGASET && ISDN_DRV_GIGASET=m"

Item    GIGASET_WASABI  boolean
Depends GIGASET_WASABI "ISDN_CAPI && FB!=y"

Item    PCI   tristate
HasPrompts  PCI   1
Item    SSB  tristate
HasPrompts  SSB  1

Item    SSB_PCIHOST_POSSIBLE    boolean
Depends SSB_PCIHOST_POSSIBLE    "SSB_POSSIBLE && SSB && (PCI || PCI=SSB)"

Item    SOME_HEX_VALUE          hex
Depends SOME_HEX_VALUE          FB

Item    SOME_VERSION            string
Depends SOME_VERSION            FB_FOOBAR

Item    SND            boolean
Item    THINKPAD_ACPI            boolean

/*
 * check-name: =n, =y, =m...
 * check-output-start
I: Items-Count: 18
I: Format: <variable> [presence condition]
UNDERTAKER_SET SCHEMA_VERSION 1.1
UNDERTAKER_SET ALWAYS_OFF "CONFIG_COMPARE_WITH_NONEXISTENT"
CONFIG_COMPARE_WITH_NONEXISTENT
CONFIG_FB "!CONFIG_FB_MODULE"
CONFIG_FB_BARFOO "(CONFIG_FB_MODULE || CONFIG_FB)"
CONFIG_FB_FOOBAR "(CONFIG_FB || (!CONFIG_FB_MODULE && !CONFIG_FB))"
CONFIG_FB_MODULE "!CONFIG_FB && CONFIG_MODULES"
CONFIG_GIGASET_CAPI "(CONFIG_ISDN && CONFIG_ISDN_DRV_GIGASET && (CONFIG_ISDN_DRV_GIGASET && CONFIG_COMPARE_WITH_NONEXISTENT))"
CONFIG_GIGASET_WASABI "((CONFIG_ISDN_CAPI_MODULE || CONFIG_ISDN_CAPI) && !CONFIG_FB)"
CONFIG_ISDN_CAPI "!CONFIG_ISDN_CAPI_MODULE"
CONFIG_ISDN_CAPI_MODULE "!CONFIG_ISDN_CAPI && CONFIG_MODULES"
CONFIG_PCI "!CONFIG_PCI_MODULE"
CONFIG_PCI_MODULE "!CONFIG_PCI && CONFIG_MODULES"
CONFIG_SND
CONFIG_SOME_HEX_VALUE "(CONFIG_FB_MODULE || CONFIG_FB)"
CONFIG_SOME_VERSION "CONFIG_FB_FOOBAR"
CONFIG_SSB "!CONFIG_SSB_MODULE"
CONFIG_SSB_MODULE "!CONFIG_SSB && CONFIG_MODULES"
CONFIG_SSB_PCIHOST_POSSIBLE "(CONFIG_SSB_POSSIBLE && (CONFIG_SSB_MODULE || CONFIG_SSB) && ((CONFIG_PCI_MODULE || CONFIG_PCI) || ((CONFIG_PCI && CONFIG_SSB) || (!CONFIG_PCI && !CONFIG_SSB && !CONFIG_PCI_MODULE && !CONFIG_SSB_MODULE) || (CONFIG_PCI_MODULE && CONFIG_SSB_MODULE))))"
CONFIG_THINKPAD_ACPI
 * check-output-end
 */

